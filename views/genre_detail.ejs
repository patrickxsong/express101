<h1>Genre: <%= genre.name %></h1>
<div style="margin-left: 20px; margin-top: 20px">
  <h4>Books</h4>
  <% if (genre_books.length !== 0) { %> <% genre_books.forEach(book => { %>
  <dl>
    <a href="<%= book.url %>"> <%= book.title %> </a>
  </dl>
  <dd><%= book.summary %></dd>
  <% }) %> <% } else { %>
  <li>This genre has no books.</li>
  <% } %>
</div>
